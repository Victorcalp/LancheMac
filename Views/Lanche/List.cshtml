@model LancheListViewModel

<div>
    <h2>@Model.CategoriaAtual</h2>
</div>

<div class="container">
    <div class="row">
        @foreach (var item in Model.Lanches)
        {
            <div class="col-md-4 col-mg-4 col-xl-4">
                <p><img src="@item.ImagemUrl" class="img-fluid"></p>
                <h3>@item.Nome</h3>
                <h4>@item.Preco.ToString("c")</h4>
                <p>@item.DescricaoCurta</p>

            </div>
        }
    </div>
</div>